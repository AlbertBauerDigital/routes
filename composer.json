{
	"name": "lms/routes",
	"type": "typo3-cms-extension",
	"description": "Provides an ability to bind a route slug to the certain Extbase Action endpoint.",
	"license": "MIT",
	"authors": [
		{
			"name": "Serhii Borulko",
			"email": "borulkosergey@icloud.com"
		}
	],
	"require": {
		"php": "^7.4",
		"typo3/cms-core": "^11.4.0",
		"lms/facade": "^1"
	},
	"autoload": {
		"psr-4": {
			"LMS\\Routes\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"LMS\\Routes\\Tests\\": "Tests"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "routes"
		}
	},
	"replace": {
		"lms/routes": "self.version",
		"typo3-ter/routes": "self.version"
	}
}
