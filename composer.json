{
	"name": "test/routes",
	"description": "Testing sandbox for <routes> extension",
	"license": "GPL-2.0-or-later",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{"type": "path", "url": "packages/*"},
		{"type": "composer", "url": "https://composer.typo3.org/"}
	],
	"config": {
		"platform": {
			"php": "7.4"
		},
		"bin-dir": "bin",
		"vendor-dir": "vendor",
		"apcu-autoloader": true,
		"optimize-autoloader": true,
		"classmap-authoritative": true,
		"preferred-install": {
			"*": "source"
		}
	},
	"require": {
		"php": "^7.4",
		"ext-json": "*",
		"ext-ctype": "*",
		"ext-iconv": "*",

		"typo3/minimal": "11.5.0",
		"typo3/cms-felogin" : "11.5.0",
		"typo3/cms-fluid-styled-content": "11.5.0",

		"lms/routes": "v11.x-dev",
		"lms/facade": "v11.x-dev"
	},
	"require-dev": {
		"vimeo/psalm": "^4.10.0",
		"typo3/testing-framework": "^6.11.3",
		"codeception/codeception": "^4.1.22",
		"codeception/module-rest": "*",
		"codeception/module-db": "*",
		"codeception/module-asserts": "*",
		"codeception/module-webdriver": "*",
		"codeception/module-phpbrowser": "*"
	},
	"autoload": {
		"psr-4": {
			"LMS\\Demo\\": "public/typo3conf/ext/demo/Classes",
			"LMS\\Routes\\Tests\\": "public/typo3conf/ext/routes/Tests"
		}
	}
}
