{
	"name": "test/routes",
	"description": "Provides an ability to bind a route slug to the certain Extbase Action endpoint.",
	"license": "MIT",
	"repositories": [
		{"type": "composer", "url": "https://composer.typo3.org/"}
	],
	"require": {
		"typo3/minimal": "^9",
		"vlucas/phpdotenv": "^2.4",

		"lms/facade": "dev-master",
		"lms/routes": "^1"
	},
	"require-dev": {
		"vimeo/psalm": "^3",
		"typo3/testing-framework": "^4",
		"codeception/codeception": "^3.1.0"
	},
	"autoload": {
		"psr-4": {
			"LMS\\Demo\\": "public/typo3conf/ext/demo/Classes"
		}
	},
	"config": {
		"sort-packages": true,
		"bin-dir": "bin",
		"vendor-dir": "vendor"
	}
}
